{
  "title": "<%= title %>",
  "description": "https://www.pivotaltracker.com/story/show/147621385",
  "graphs": [
    {
      "definition": {
        "viz": "timeseries",
        "status": "done",
        "requests": [
          {
            "q": "anomalies(max:<%= prefix %>.grootfs.dstate_processes{<%= grootfs_query %>}.rollup(max, 1), 'basic', 1)",
            "aggregator": "avg",
            "conditional_formats": [],
            "type": "line"
          }
        ],
        "autoscale": true
      },
      "title": "GROOTFS: DSTATE Processes"
    },
