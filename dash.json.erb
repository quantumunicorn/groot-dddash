    {
      "definition": {
        "viz": "timeseries",
        "requests": [
          {
            "q": "anomalies(max:<%= prefix %>.grootfs.ImageCreationTime{<%= query %>}.rollup(max, 1), 'adaptive', 1)",
            "aggregator": "avg",
            "conditional_formats": [],
            "type": "line"
